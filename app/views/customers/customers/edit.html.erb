<div class="container ">
  <h3 class="row"><span class="bg-info">登録情報編集</span></h3>
    <%= form_for(@customer ,url:customers_path,method: :patch, html: { class: "form-horizontal", novalidate: "novalidate" }) do |f| %>
      <%= render "customers/customer_form_item",f:f %>
        <%= f.submit "編集内容を保存する", class:"btn btn-success" %>
        <%= link_to '退会する', unsubscribe_customer_path, class: "btn btn-danger" %>
      </div>
    <% end %>
  </div>
</div>