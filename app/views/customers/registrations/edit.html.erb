<div class="container ">
  <h3 class="row"><span class="bg-info">登録情報編集</span></h3>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { class: "form-horizontal", novalidate: "novalidate" }) do |f| %>
      <%= render partial: "customer_form_item", locals: {resource: resource, f: f} %>
        <%= f.submit "編集内容を保存する", class:"btn btn-success" %>
        <%= link_to '退会する', unsubscribe_customer_path, class: "btn btn-danger" %>
      </div>
    <% end %>
  </div>
</div>